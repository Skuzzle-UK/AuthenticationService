{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MySQL": "${ConnectionStrings__MySQL}"
  },
  "AdminAccountSeedSettings": {
    "Email": "${AdminAccountSeedSettings__Email}",
    "Password": "${AdminAccountSeedSettings__Password}",
    "FirstName": "Administrator",
    //"LastName": "",
    //"PhoneNumber": "",
    //"PhoneNumberConfirmed": false,
    "Country": "United Kingdom"
  },
  "JWTSettings": {
    "SecurityKey": "${JWTSettings__SecurityKey}",
    "ValidIssuer": "${JWTSettings__ValidIssuer}",
    "ValidAudience": "${JWTSettings__ValidAudience}",
    "ExpiryInMinutes": 5
  },
  "EmailServerSettings": {
    "From": "${EmailServerSettings__From}",
    "SmtpServer": "${EmailServerSettings__SmtpServer}",
    "Port": "${EmailServerSettings__Port}",
    "UserName": "${EmailServerSettings__UserName}",
    "Password": "${EmailServerSettings__Password}"
  },
  "AllowedHosts": "*"
}
